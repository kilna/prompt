#!/usr/bin/env bash

red::module::error() {

  (( "$RED_LAST_EXIT" )) || return

  red::set module_error_content "$RED_LAST_EXIT"
  red::set module_error_color '#F00'
  red::set module_error_prefix_color '#FF0'
  red::set module_error_prefix_unicode 'âš '
  red::enable module_error_prefix_unicode_wide
  red::set module_error_prefix_ascii "err"

}

